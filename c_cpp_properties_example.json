{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}\\libs\\json\\*",
                "${workspaceFolder}\\include\\*"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "WIN32",
                "GLEW_BUILD"
            ],
            "intelliSenseMode": "windows-clang-x86",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/build/*",
                    "${workspaceFolder}/libs",
                    "${workspaceFolder}/include",
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "cStandard": "c17",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerPath": "C:/development/LLVM/bin/clang++.exe"
        }
    ],
    "version": 4
}